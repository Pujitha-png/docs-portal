{
  "openapi": "3.0.3",
  "info": {
    "title": "Docs Portal API",
    "version": "1.0.0",
    "description": "Sample API specification for Swagger UI integration."
  },
  "servers": [
    {
      "url": "https://api.example.com"
    }
  ],
  "paths": {
